package Skills
import NoWurst

public abstract class ISkill
    protected unit m_self_unit = null
    protected trigger m_tr_self = null
    construct(unit u)
        m_self_unit = u
        m_tr_self = CreateTrigger()

    ondestroy
        DisableTrigger(m_tr_self)
        DestroyTrigger(m_tr_self)

    abstract function cast()
        
    abstract function listen()

    abstract function get_self_id() returns int
